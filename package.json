{
  "name": "LIFECHANGE",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "start": "npx react-native start --reset-cache",
    "android": "npx react-native run-android",
    "make-apk": "cd android && gradlew assembleRelease --stacktrace",
    "make-bundle": "cd android && gradlew bundleRelease --stacktrace",
    "build-clean": "cd android && gradlew clean --stacktrace",
    "resolve-load": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "kill-java": "taskkill /f /im java.exe",
    "git-push": "git add . && git commit -m 'update' && git push origin main",
    "npm-reset": "rd /s /q node_modules && del /f package-lock.json && npm cache clean --force && npm install",
    "npm-install": "npm install",
    "npm-clean": "npm cache clean --force"
  },
  "dependencies": {
    "axios": "^1.7.5",
    "moment-timezone": "^0.5.45",
    "react": "18.2.0",
    "react-native": "0.74.3",
    "react-native-android-widget": "^0.14.1",
    "react-native-gesture-handler": "^2.18.1",
    "react-native-google-mobile-ads": "^14.2.3",
    "react-native-localize": "^3.2.1",
    "react-native-screens": "^3.34.0",
    "react-native-webview": "^13.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/runtime": "^7.25.6",
    "@react-native/eslint-config": "0.75.2",
    "@react-native/metro-config": "0.74.85",
    "@react-native/typescript-config": "0.75.2",
    "@types/react": "^18.3.5",
    "@types/react-test-renderer": "^18.2.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.9.1",
    "jest": "^29.7.0",
    "prettier": "3.3.3",
    "react-test-renderer": "18.2.0",
    "typescript": "5.5.4"
  }
}
