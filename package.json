{
  "name": "LIFECHANGE",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "start": "npx react-native start --reset-cache",
    "android": "npx react-native run-android",
    "make-apk": "cd android && gradlew assembleRelease --stacktrace",
    "make-bundle": "cd android && gradlew bundleRelease --stacktrace",
    "make-apk&bundle": "cd android && gradlew bundleRelease && gradlew assembleRelease --stacktrace",
    "build-clean": "cd android && gradlew clean --stacktrace",
    "resolve-load": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "kill-java": "taskkill /f /im java.exe",
    "push": "git add . && git commit -m 'update' && git push origin main"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.23.1",
    "axios": "^1.7.2",
    "moment-timezone": "^0.5.45",
    "react": "18.2.0",
    "react-native": "0.74.3",
    "react-native-android-widget": "^0.14.1",
    "react-native-gesture-handler": "^2.17.1",
    "react-native-google-mobile-ads": "^13.6.0",
    "react-native-screens": "^3.32.0",
    "react-native-webview": "^13.10.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.85",
    "@react-native/eslint-config": "0.74.85",
    "@react-native/metro-config": "0.74.85",
    "@react-native/typescript-config": "0.74.85",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  }
}
