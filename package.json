{
  "name": "LIFECHANGEd_RN",
  "version": "1.0.0",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "start": "npx react-native start --reset-cache",
    "build": "cd android && gradlew clean && gradlew assembleRelease --stacktrace",
    "make-apk": "cd android && gradlew assembleRelease --stacktrace",
    "make-bundle": "cd android && gradlew bundleRelease --stacktrace",
    "kill-java": "taskkill /f /im java.exe",
    "npm-reset": "powershell -Command \"Remove-Item -Recurse -Force package-lock.json, node_modules; npm install --legacy-peer-deps;\"",
    "gcloud": "node gcloud.cjs"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.0",
    "axios": "^1.7.9",
    "moment-timezone": "^0.5.46",
    "react": "18.3.1",
    "react-native": "0.74.6",
    "react-native-android-widget": "^0.14.2",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "^2.21.2",
    "react-native-google-mobile-ads": "^14.7.2",
    "react-native-screens": "^4.4.0",
    "react-native-webview": "^13.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-private-methods": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/runtime": "^7.26.0",
    "@react-native/eslint-config": "0.76.5",
    "@react-native/metro-config": "0.76.5",
    "@react-native/typescript-config": "0.76.5",
    "@types/react": "^18.3.18",
    "@types/react-test-renderer": "^18.3.1",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "3.4.2",
    "react-test-renderer": "18.3.1",
    "typescript": "5.7.2"
  }
}